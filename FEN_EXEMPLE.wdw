     name : SurElémentAjouté
     procedure_id : 1341042966252429770
     type_code : 14
     code : |1-
      // Résumé : Procédure WLangage associée au délégué DOTNET
      // Syntaxe :
      //SurElémentAjouté (<pclObjet>, <pclEvénement> est objet .NET)
      //
      // Paramètres :
      //	pclObjet : Objet DOTNET source de l'événement DOTNET
      //	pclEvénement (objet .NET) : Données de l'événement DOTNET (dans notre exemple, du type FileSystemEventArgs)
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SurElémentAjouté(pclObjet <utile>, pclEvénement est un FileSystemEventArgs dynamique)
      
      	sNomElément est une chaine
      	
      	// Récupère le nom de l'élément créé
      	sNomElément = pclEvénement:FullPath
      	
      	// On affiche l'information
      	si EnModeTestAutomatique() alors
      		trace(chaineconstruit("L'élément <%1> a été créé", sNomElément))
      	sinon
      		infoconstruit("L'élément <%1> a été créé", sNomElément)
      	FIN
      	
      	// Pour l'exemple, on supprime immédiatement le fichier
      	fsupprime(sNomElément)
     type : 458752
   -
     name : DonneNouveauNomFichier
     procedure_id : 1341049537552481014
     type_code : 14
     code : |1-
      // Résumé : Génère un nom de fichier unique pour un répertoire
      // Syntaxe :
      //[ <Résultat> = ] DonneNouveauNomFichier (<sDossier> est chaîne)
      //
      // Paramètres :
      //	sDossier (chaîne ANSI) : Dossier racine
      // Valeur de retour :
      // 	chaîne ANSI : Nom de fichier généré
      //
      PROCEDURE DonneNouveauNomFichier(local sDossier est une chaine)
      
      	sFichier est une chaine = "Fichier_test_" + DonneIdentifiant()
      	
      	// Si le fichier existe déjà
      	tantque fFichierExiste(sDossier + ["\"] + sFichier)
      		
      		// On recommence avec un nouveau nom
      		sFichier = "Fichier_test_" + DonneIdentifiant()
      	FIN
      	
      renvoyer sDossier + ["\"] + sFichier
     type : 458752
